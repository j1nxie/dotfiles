{
  "workbench.colorTheme": "Catppuccin Macchiato",
  "security.workspace.trust.untrustedFiles": "open",
  "editor.fontFamily": "'FiraCode Nerd Font', 'Fira Code', Consolas, 'Courier New', monospace",
  "editor.renderWhitespace": "all",
  "[python]": {
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
      "source.fixAll": "explicit"
    },
    "editor.defaultFormatter": "charliermarsh.ruff"
  },
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "eslint.validate": [
    "javascript",
    "typescript"
  ],
  "editor.fontVariations": true,
  "editor.formatOnSave": true,
  "editor.fontWeight": "normal",
  "editor.fontLigatures": true,
  "rust-analyzer.check.command": "clippy",
  "terminal.external.linuxExec": "kitty",
  "terminal.integrated.fontWeight": "normal",
  "editor.guides.bracketPairs": true,
  "vscode-neovim.neovimInitVimPaths.linux": "/home/lumi/.config/nvim/vscode.lua",
  "editor.unicodeHighlight.nonBasicASCII": false,
  "vscode-neovim.highlightGroups.unknownHighlight": "vim",
  "extensions.experimental.affinity": {
    "asvetliakov.vscode-neovim": 1
  },
  "editor.lineNumbers": "relative",
  "python.formatting.provider": "none",
  "vscord.app.name": "Visual Studio Code",
  "vscode-neovim.useWSL": true,
  "workbench.iconTheme": "catppuccin-macchiato",
  "todohighlight.isEnable": true,
  "todohighlight.include": [
    "**/*.js",
    "**/*.jsx",
    "**/*.ts",
    "**/*.tsx",
    "**/*.html",
    "**/*.css",
    "**/*.scss",
    "**/*.php",
    "**/*.rb",
    "**/*.txt",
    "**/*.mdown",
    "**/*.md",
    "**/*.rs",
    "**/*.java"
  ],
  "godot_tools.gdscript_lsp_server_port": 6005,
  "window.menuBarVisibility": "toggle",
  "workbench.settings.useSplitJSON": true,
  "editor.inlayHints.fontSize": 12,
  "editor.inlayHints.padding": true,
  "editor.smoothScrolling": true,
  "terminal.integrated.tabFocusMode": true,
  "search.showLineNumbers": true,
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.cursorBlinking": true,
  "problems.showCurrentInStatus": true,
  "editor.accessibilitySupport": "off",
  "git.openRepositoryInParentFolders": "never",
  "C_Cpp.autocompleteAddParentheses": true,
  "[c]": {
    "editor.defaultFormatter": "ms-vscode.cpptools"
  },
  "C_Cpp.inlayHints.parameterNames.enabled": true,
  "C_Cpp.inlayHints.autoDeclarationTypes.showOnLeft": true,
  "C_Cpp.inlayHints.autoDeclarationTypes.enabled": true,
  "C_Cpp.inlayHints.referenceOperator.enabled": true,
  "C_Cpp.inlayHints.referenceOperator.showSpace": true,
  "C_Cpp.codeAnalysis.clangTidy.enabled": true,
  "cmake.configureOnOpen": true,
  "redhat.telemetry.enabled": true,
  "latex-workshop.latex.recipe.default": "tectonic",
  "latex-workshop.latex.autoBuild.run": "onSave",
  "latex-workshop.view.pdf.viewer": "tab",
  "latex-workshop.latex.recipes": [
    {
      "name": "tectonic",
      "tools": [
        "tectonic"
      ]
    }
  ],
  "latex-workshop.latex.tools": [
    {
      "name": "tectonic",
      "command": "tectonic",
      "args": [
        "-X",
        "build",
        "--keep-intermediates",
        "--keep-logs"
      ],
      "env": {}
    }
  ],
  "javascript.inlayHints.enumMemberValues.enabled": true,
  "explorer.fileNesting.enabled": true,
  "editor.cursorSmoothCaretAnimation": "on",
  "csharp.maxProjectFileCountForDiagnosticAnalysis": 0,
  "dotnet.inlayHints.enableInlayHintsForIndexerParameters": true,
  "dotnet.inlayHints.enableInlayHintsForParameters": true,
  "dotnet.inlayHints.enableInlayHintsForOtherParameters": true,
  "dotnet.inlayHints.suppressInlayHintsForParametersThatMatchArgumentName": true,
  "dotnet.inlayHints.suppressInlayHintsForParametersThatMatchMethodIntent": true,
  "dotnet.inlayHints.enableInlayHintsForObjectCreationParameters": true,
  "workbench.preferredDarkColorTheme": "Catppuccin Macchiato",
  "workbench.preferredLightColorTheme": "Catppuccin Latte",
  "lldb.library": "/usr/lib/swift/lib/liblldb.so",
  "lldb.launch.expressions": "native",
  "svelte.enable-ts-plugin": true,
  "window.autoDetectColorScheme": true,
  "workbench.editor.enablePreview": false,
  "vue.server.runtime": "bun",
  "ruff.enableExperimentalFormatter": true,
  "apc.electron": {
    "frame": false
  },
  "[typescript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "npm.packageManager": "pnpm",
  "eslint.packageManager": "pnpm",
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "workbench.startupEditor": "none",
  "typescript.inlayHints.functionLikeReturnTypes.enabled": true,
  "typescript.inlayHints.variableTypes.enabled": true,
  "typescript.inlayHints.propertyDeclarationTypes.enabled": true,
  "typescript.inlayHints.parameterTypes.enabled": true,
  "typescript.inlayHints.parameterNames.enabled": "all",
  "typescript.inlayHints.enumMemberValues.enabled": true,
  "[javascript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "files.associations": {
    "*.css": "tailwindcss"
  },
  "editor.codeActionWidget.includeNearbyQuickFixes": true,
  "remote.SSH.remotePlatform": {
    "100.91.189.122": "linux"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "editor.hideCursorInOverviewRuler": true,
  "[markdown]": {
    "editor.rulers": [
      80
    ]
  },
  "[latex]": {
    "editor.rulers": [
      80
    ]
  },
  "zig.path": "",
  "zig.zls.path": "/home/lumi/.config/Code/User/globalStorage/ziglang.vscode-zig/zls_install/zls",
  "zig.initialSetupDone": true,
  "prettier.tabWidth": 4,
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "editor.stickyScroll.enabled": true
}