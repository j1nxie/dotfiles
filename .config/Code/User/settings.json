{
    "workbench.colorTheme": "Catppuccin Latte",
    "security.workspace.trust.untrustedFiles": "open",
    "editor.fontFamily": "'FiraCode Nerd Font', Consolas, 'Courier New', monospace",
    "editor.renderWhitespace": "all",
    "[python]": {
        "editor.formatOnSave": true,
        "editor.codeActionsOnSave": {
            "source.fixAll": true
        },
        "editor.defaultFormatter": "ms-python.black-formatter"
    },
    "editor.fontVariations": true,
    "editor.formatOnSave": true,
    "editor.fontWeight": "normal",
    "editor.fontLigatures": true,
    "rust-analyzer.check.command": "clippy",
    "terminal.external.linuxExec": "kitty",
    "terminal.integrated.fontWeight": "normal",
    "editor.guides.bracketPairs": "active",
    "vscode-neovim.neovimInitVimPaths.linux": "/home/lumi/.config/nvim/vscode.lua",
    "editor.unicodeHighlight.nonBasicASCII": false,
    "vscode-neovim.highlightGroups.unknownHighlight": "vim",
    "extensions.experimental.affinity": {
        "asvetliakov.vscode-neovim": 1
    },
    "editor.lineNumbers": "relative",
    "python.formatting.provider": "none",
    "vscord.app.name": "Visual Studio Code",
    "vscode-neovim.useWSL": true,
    "workbench.iconTheme": "catppuccin-latte",
    "todohighlight.isEnable": true,
    "todohighlight.include": [
        "**/*.js",
        "**/*.jsx",
        "**/*.ts",
        "**/*.tsx",
        "**/*.html",
        "**/*.css",
        "**/*.scss",
        "**/*.php",
        "**/*.rb",
        "**/*.txt",
        "**/*.mdown",
        "**/*.md",
        "**/*.rs",
        "**/*.java"
    ],
    "godot_tools.gdscript_lsp_server_port": 6005,
    "window.menuBarVisibility": "toggle",
    "workbench.settings.useSplitJSON": true,
    "editor.inlayHints.fontSize": 12,
    "editor.inlayHints.padding": true,
    "editor.smoothScrolling": true,
    "editor.stickyScroll.enabled": true,
    "terminal.integrated.tabFocusMode": true,
    "search.showLineNumbers": true,
    "terminal.integrated.cursorStyle": "line",
    "terminal.integrated.cursorBlinking": true,
    "problems.showCurrentInStatus": true,
    "editor.accessibilitySupport": "off",
    "git.openRepositoryInParentFolders": "never",
    "C_Cpp.autocompleteAddParentheses": true,
    "[c]": {
        "editor.defaultFormatter": "ms-vscode.cpptools"
    },
    "C_Cpp.inlayHints.parameterNames.enabled": true,
    "C_Cpp.inlayHints.autoDeclarationTypes.showOnLeft": true,
    "C_Cpp.inlayHints.autoDeclarationTypes.enabled": true,
    "C_Cpp.inlayHints.referenceOperator.enabled": true,
    "C_Cpp.inlayHints.referenceOperator.showSpace": true,
    "C_Cpp.codeAnalysis.clangTidy.enabled": true,
    "cmake.configureOnOpen": true,
    "redhat.telemetry.enabled": true,
    "texlab.build.executable": "tectonic",
    "texlab.build.args": [
        "-X",
        "compile",
        "%f",
        "--synctex",
        "--keep-logs",
        "--keep-intermediates"
    ],
    "texlab.build.onSave": true,
    "javascript.inlayHints.enumMemberValues.enabled": true,
    "explorer.fileNesting.enabled": true,
    "editor.cursorSmoothCaretAnimation": "on",
    "csharp.maxProjectFileCountForDiagnosticAnalysis": 0,
    "dotnet.inlayHints.enableInlayHintsForIndexerParameters": true,
    "dotnet.inlayHints.enableInlayHintsForParameters": true,
    "dotnet.inlayHints.enableInlayHintsForOtherParameters": true,
    "dotnet.inlayHints.suppressInlayHintsForParametersThatMatchArgumentName": true,
    "dotnet.inlayHints.suppressInlayHintsForParametersThatMatchMethodIntent": true,
    "dotnet.inlayHints.enableInlayHintsForObjectCreationParameters": true,
    "workbench.preferredDarkColorTheme": "Catppuccin Macchiato",
    "workbench.preferredLightColorTheme": "Catppuccin Latte",
    "lldb.library": "/usr/lib/swift/lib/liblldb.so",
    "lldb.launch.expressions": "native",
    "svelte.enable-ts-plugin": true,
    "window.autoDetectColorScheme": true,
    "workbench.editor.enablePreview": false,
    "window.titleBarStyle": "custom",
    "vue.server.runtime": "bun",
    "ruff.enableExperimentalFormatter": true,
}